{
    "name": "webmunkeez/i18n-bundle",
    "type": "symfony-bundle",
    "description": "Internationalization for Symfony.",
    "keywords": [
        "symfony",
        "autowiring",
        "internationalization",
        "language"
    ],
    "homepage": "https://github.com/yannissgarra/i18n-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Sgarra",
            "email": "hello@yannissgarra.com",
            "homepage": "https://yannissgarra.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/expression-language": "^6.2",
        "symfony/intl": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/serializer": "^6.2",
        "symfony/string": "^6.2",
        "symfony/translation": "^6.2",
        "symfony/uid": "^6.2",
        "symfony/validator": "^6.2",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.2",
        "symfony/css-selector": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Webmunkeez\\I18nBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmunkeez\\I18nBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
